#include<stdio.h>
#include<conio.h>
#include <math.h>

void linha()
{
	int i;
	for(i = 0; i<20; i++)
	{
		printf("*");
		
	}
	printf("\n");
	
}
int main()
{
	int i;
	linha();
	
	printf("numeros entre 1 a 5\n");
	
	for(i = 1; i<20; i++)
	{
		printf("*");
	}	
	
	printf("\n");
	
	for(i = 0; i <= 5; i++)
	{
		printf("%d\n", i);
	}
	linha();
	printf("\n");
}
